package antgame;

/**
 *
 * @author ap487
 */
public class AntHill {

    private Color color;
    private Cell cell;

    public enum Color {
        RED,
        BLACK
    }

    public AntHill(Cell cell, Color color) {
        this.cell = cell;
        this.color = color;
    }

    public Color getColor() {
        return color;
    }

    public Cell getCell() {
        return cell;
    }
}
